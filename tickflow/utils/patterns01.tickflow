sequenceJackson:
	set_cat 0
	setrest 7, 0x300
	call changeEngine
	engine 0x3D
	rest 0x30
    0x10D<1>
    0x10E 1
    0x10C 0, -0x320, 0x5DC
	0xB3 9, 0x3D

	fade<1> 1,1,0x30
    async_call playJackson
    async_call moonWalk
    input 1
    async_sub 0x64
    rest 0xC0
    async_sub 0x64
    rest 0xC0
    async_sub 0x64
    rest 0xC0
    async_sub 0x5A
stop