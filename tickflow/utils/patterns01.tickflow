sickBeat:
    label 0xBEA7
    0x107
    rest 0x30
    goto 0xBEA7
stop

sequenceBannerSickBeats:
    setrest 7, 0xC0
	0xB3 9, 0x40

    input 1
    async_call sickBeat
    async_sub 0x56
    async_sub 0x59
    rest 0x30
    rest 0x30
    async_call playBanner

stop

sequenceWii:
	set_cat 0
	setrest 7, 0x210
	call changeEngine
	engine 0x28
	rest 0x30
	0xB3 9, 0x28

	fade<1> 1,1,0x30
    async_call playWii

    input 1
    0x10D 0
    rest 0xC
    0x100 0
    rest 0x48
    0x100 1
    rest 0x30
    async_sub 0x6D
    rest 0x90
    async_sub 0x73
stop

beatIt:
