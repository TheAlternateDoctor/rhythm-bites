playNextGame:
	0xB3 7, 0 //Resets screen fade
    0x21 120
    get_async 1, 0
    rest 0x30
    sub 3
	get_async 5, 0
    get_sync 2
    get_async 0, 0
	rest 0x30
	async_call loadNextGame,0x60
	getrest 7
    rest 0x60
	call fade
    call lifeCheck
    // get_async 3, 0
return

switchSickBeats:
	set_cat 0
    call changeEngine
    engine 0x40
    rest 0x30
    0xAF<6> 0
	fade<1> 1,1,0x30
    0xB0<4> 0xA, "docShorts_song_banner_megamix"
    setrest 8, 0x90
    async_call songDisappear
return

switchCoinToss:
	set_cat 0
    call changeEngine
    engine 0x41
    rest 0x30
    0xAF<6> 0
	fade<1> 1,1,0x30
    0xB0<4> 0xA, "docShorts_song_banner_megamix"
    setrest 8, 0xC0
    async_call songDisappear
return

switchClapTrap:
	set_cat 0
    call changeEngine
    engine 0x42
    rest 0x30
    0xAF<6> 0
	fade<1> 1,1,0x30
    0xB0<4> 0xA, "docShorts_song_banner_megamix"
    setrest 8, 0x60
    async_call songDisappear
return

deadSubEndless: //Just save, but then continue
	0x2D 0
	0x21 120
	input 0
	set_cat 0
	0x76 1, 1	// Which screen to affect
	0x77 1, 0xFF	// Immediate opacity level
	0x78 1, 0,0,0	// Color, RGB
	0x79 1, 0	// Depth of the screen.
	engine 0x43
	0xAF<6> 1
	rest 0xC
	0xAF<6> 0
	fade<1> 1, 1, 1
	
	0xB2 0
	rest 0x18
	0x10D<1>

	0x2D 1
	rest 0x18	// 1.0 beats
	speed 0x100
	0xAF<2> 1, 0, 0x50DFC6
	play_sfx 0x1000AB7
	rest 0xC0	// 4.0 beats
	rest 0xC0	// 4.0 beats
	fade 1, 0, 0x60
	rest 0x60
	set_func 5, nop
	set_func 4, nopR
	0x67<2>
    0x77 1, 0x0
return

nop:
